[access "refs/*"]
    read = group Anonymous Users
    read = group Registered Users
    read = group Non-Interactive Users
    read = group mirantis
[access "refs/for/refs/*"]
    pushMerge = group mcp-ci-core
[access "refs/heads/*"]
    abandon = group Change Owner
    abandon = group calico-core
    abandon = group mcp-ci-core
    create = group calico-core
    create = group mcp-ci-core
    label-Code-Review = -2..+2 group calico-core
    label-Code-Review = -2..+2 group mcp-ci-core
    label-Code-Review = -1..+1 group Registered Users
    label-Verified = -2..+2 group Non-Interactive Users
    label-Workflow = -1..+0 group Change Owner
    label-Workflow = -1..+1 group calico-core
    label-Workflow = -1..+1 group mcp-ci-core
    rebase = group calico-core
    rebase = group mcp-ci-core
    rebase = group Change Owner
    submit = group calico-core
    submit = group mcp-ci-core
[access "refs/meta/*"]
    read = group Non-Interactive Users
    read = group mirantis
[access "refs/meta/config"]
    read = group Non-Interactive Users
    read = group mirantis
[receive]
    requireChangeId = true
[submit]
    mergeContent = true
[access]
    inheritFrom = All-Projects
